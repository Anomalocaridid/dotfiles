#~/.customizepkg/nnn-nerd
# Custon nnn install

# add pcre library
add#depends#pcre

# add custom options
replace#build#make#make O_PCRE=1
replace#build#make#make O_NOBATCH=1
replace#build#make#make O_NOUG=1
replace#build#make#make O_QSORT=1
replace#build#make#make O_CTX8=1
replace#build#make#make O_GITSTATUS=1
replace#build#make#make O_NAMEFIRST=1
